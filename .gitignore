# Cache and logs (Symfony2)
/src/app/cache/*
/src/app/logs/*
/src/!app/cache/.gitkeep
/src/!app/logs/.gitkeep

# Email spool folder
/src/app/spool/*

# Cache, session files and logs (Symfony3)
/src/var/cache/dev/*
/src/var/cache/*
/src/var/logs/*
/src/var/sessions/*
/src/!var/cache/.gitkeep
/src/!var/logs/.gitkeep
/src/!var/sessions/.gitkeep

# Logs (Symfony4)
/src/var/log/*
!/src/var/log/.gitkeep

# Parameters
/src/app/config/parameters.yml
/src/app/config/parameters.ini

# Managed by Composer
/src/app/bootstrap.php.cache
/src/var/bootstrap.php.cache
/src/vendor/

# Assets and user uploads
/src/web/bundles/
/src/web/uploads/

# PHPUnit
/src/app/phpunit.xml
/src/phpunit.xml

# Build data
/src/build/

# Composer PHAR
/composer.phar

# Embedded web-server pid file
/.web-server-pid
.env
